<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.qubership.automation.itf.core.model.jpa.migration.EntitiesMigration"
           table="entities_migration">
        <!--Immutable section starts-->
        <id name="id" type="java.util.UUID">
            <column name="id" sql-type="uuid"/>
        </id>
        <property name="date" type="java.util.Date" column="date"/>
        <property name="status" type="java.lang.String" column="status"/>
        <property name="description" type="java.lang.String" column="description"/>
    </class>

</hibernate-mapping>
